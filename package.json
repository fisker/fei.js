{
  "name": "fei.js",
  "version": "1.1.0",
  "description": "image preprocessor",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "browser": "lib/index.min.js",
  "unpkg": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "repository": "git@github.com:fisker/fei.js.git",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "scripts": {
    "dev": "poi --serve --watch --debug --open",
    "build": "npm-run-all build:*",
    "build:js": "rollup --config",
    "build:demo": "poi --prod",
    "build:fixture": "cp -r __tests__/fixture docs/fixture",
    "dist": "npm-run-all dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "clean": "rimraf docs lib",
    "test-coverage": "jest --coverage",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src/**/*.js --fix",
    "release": "run-s clean lint build test dist",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": ["lib"],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-jest": "^24.7.1",
    "babel-plugin-transform-async-to-promises": "^0.8.8",
    "canvas": "^2.4.1",
    "core-js": "^3.0.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-fisker": "^4.3.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-environment-jsdom-thirteen": "^1.0.1",
    "jpeg-buffer-orientation": "^1.0.0",
    "jsdom": "^14.0.0",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.14.1",
    "mem": "^4.3.0",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "poi": "^12.6.1",
    "prettier": "^1.16.4",
    "promisify-file-reader": "^3.0.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-terser": "^4.0.4"
  }
}
