{
  "name": "fei.js",
  "version": "2.2.1",
  "description": "image preprocessor",
  "homepage": "https://github.com/fisker/fei.js#readme",
  "bugs": {
    "url": "https://github.com/fisker/fei.js/issues"
  },
  "repository": "fisker/fei.js",
  "funding": "https://github.com/fisker/fei.js?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dev": "poi --serve --watch --debug --open",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@commitlint/cli": "12.1.4",
    "@fisker/commitlint-config": "1.3.5",
    "@fisker/eslint-config": "6.0.3",
    "@fisker/eslint-config-jest": "2.0.9",
    "@fisker/husky-config": "4.0.5",
    "@fisker/lint-staged-config": "3.0.5",
    "@fisker/prettier-config": "4.0.5",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "babel-jest": "27.0.6",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "canvas": "2.8.0",
    "core-js": "3.15.1",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "4.0.0",
    "eslint": "7.29.0",
    "husky": "6.0.0",
    "jest": "27.0.6",
    "jpeg-buffer-orientation": "3.0.1",
    "jsdom": "16.6.0",
    "lint-staged": "11.0.0",
    "markdownlint-cli": "0.27.1",
    "mem": "8.1.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.3.2",
    "promisify-file-reader": "4.0.2",
    "rollup": "2.52.3",
    "rollup-plugin-prettier": "2.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sort-package-json": "1.50.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
