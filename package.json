{
  "name": "fei.js",
  "version": "1.1.1",
  "description": "image preprocessor",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "browser": "lib/index.min.js",
  "unpkg": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "repository": "git@github.com:fisker/fei.js.git",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "scripts": {
    "dev": "poi --serve --watch --debug --open",
    "build": "npm-run-all build:*",
    "build:js": "rollup --config",
    "build:demo": "poi --prod",
    "build:fixture": "cp -r __tests__/fixture docs/fixture",
    "dist": "npm-run-all dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "clean": "rimraf docs lib",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint **/*.{js,mjs,vue} --fix",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "release": "run-s clean lint build test prettier dist",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^7.6.1",
    "@fisker/commitlint-config": "^1.0.8",
    "@fisker/eslint-config": "^1.0.14",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-async-to-promises": "^0.8.10",
    "canvas": "^2.5.0",
    "core-js": "^3.1.2",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-fisker": "^4.5.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-environment-jsdom-thirteen": "^1.0.1",
    "jpeg-buffer-orientation": "^1.0.1",
    "jsdom": "^15.1.0",
    "lint-staged": "^8.1.7",
    "markdownlint-cli": "^0.16.0",
    "mem": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "poi": "^12.6.8",
    "prettier": "^1.17.1",
    "promisify-file-reader": "^3.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-terser": "^5.0.0"
  }
}
